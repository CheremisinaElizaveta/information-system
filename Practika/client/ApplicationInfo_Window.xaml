<Window x:Class="Practika.ApplicationInfo_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practika"
        mc:Ignorable="d"
        Title="ApplicationInfo_Window" Height="450" Width="800"
        Background="#1A1A1A">
    <Grid>
        <DataGrid x:Name="ApplicationInfo_W" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="28,66,28,227" Background="#FF321709"/>
        <TextBlock HorizontalAlignment="Left" Margin="28,16,0,0" TextWrapping="Wrap" Text="Заявки" VerticalAlignment="Top" Foreground="White" FontSize="30"/>

    </Grid>
</Window>
